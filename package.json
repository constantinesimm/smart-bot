{
  "name": "smart-bot",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "npm run build && node ./bin/www",
    "dev": "nodemon  ./bin/www",
    "dev:app": "concurrently \"npm run dev\" \"npm run serve\"",
